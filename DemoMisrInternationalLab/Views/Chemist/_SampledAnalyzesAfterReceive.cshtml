@model DemoMisrInternationalLab.Models.PatientRequestAnalysisViewModel

@if (Model.PatientRequestAnalyzes != null)
{
    for (int i = 0; i < Model.PatientRequestAnalyzes.Count; i++)
    {

        <div class="ck-buttonb">
            <label>

                <span>
                    <table width="100%">

                        <tr>
                            <td width="8%">@Html.DisplayTextFor(m => Model.PatientRequestAnalyzes[i].RequestNumber)</td>
                            <td width="82%">@Html.DisplayFor(x => Model.PatientRequestAnalyzes[i].AnalysisName)</td>
                            <td width="10%"></td>
                        </tr>

                    </table>
                </span>
            </label>
        </div>
    }
}

